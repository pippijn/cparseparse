# Target
Name		= cpapa_js

# Sources
OCaml-Sources[] =
  Cpapa_js

# Library dependencies
OCaml-Requires[] =
  cpapa-dynparse
  glr
  js_intf

# Only byte-code
OCAML_BYTE = true
OCAML_NATIVE = false

upload: $(Name).js
  chmod 644 $^
  scp $^ ra:public_html/files/up/parser/

TARGET = $(program)
.DEFAULT: $(TARGET)
